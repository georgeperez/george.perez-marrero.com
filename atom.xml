---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>George Perez-Marrero</title>
 <link href="http://georgeperezmarrero.com/atom.xml" rel="self"/>
 <link href="http://georgeperezmarrero.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://georgeperezmarrero.com/</id>
 <author>
   <name>George Perez-Marrero</name>
   <email>george@georgeperezmarrero.com</email>
 </author>

 {% for post in site.posts %}
 {% if post.external-url %}
 <entry>
  <title>{{ post.title }}</title>
 <link href="{{ post.external-url }}"/>
 <updated>{{ post.date | date_to_xmlschema }}</updated>
 <id>http://georgeperezmarrero.com{{ post.id }}</id>
 <content type="html">{{ post.content | xml_escape }}</content>
</entry>
{% else %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://georgeperezmarrero.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://georgeperezmarrero.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endif %}
 {% endfor %}

</feed>